[user]
	name = Jan Trojak
	email = trojaj12@fit.cvut.cz
[color]
	ui = auto
[push]
	default = simple
[core]
	editor = nano
	autocrlf = input
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	ll = log --oneline --graph --all --decorate
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[submodule]
	recurse = true
[pull]
	rebase = false
[init]
	defaultBranch = master
